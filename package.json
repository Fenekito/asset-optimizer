{
  "name": "@fenekito/asset-optimizer",
  "version": "1.2.0",
  "description": "A package for optimizing images and video files for improved performance.",
  "keywords": [
    "media",
    "convert",
    "webp",
    "png",
    "jpg",
    "webm",
    "mp4",
    "asset",
    "image",
    "video",
    "minify",
    "optimize",
    "performance"
  ],
  "license": "MIT",
  "author": "Juan Claudio Caires Pereira <juanccaires@gmail.com>",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "asset-optimizer": "dist/cli.cjs"
  },
  "files": [
    "dist",
    "README.md",
    "types"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "start": "node dist/cli.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/ui": "^4.0.4",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.4"
  },
  "dependencies": {
    "sharp": "^0.34.4",
    "svgo": "^4.0.0"
  },
  "optionalDependencies": {
    "mediabunny": "^1.24.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
